<h1>Add new Tweet</h1>

<%= form_for @tweets, :url => { :action => "create" }, :html => {:class => "nifty_form"} do |f| %>
  <%= f.label :tweet %>:
  <%= f.text_field :tweet %><br />
  <%= f.label :userId %>:
  <%= f.text_field :userId %><br />
  <%= f.submit %>
<% end %>


