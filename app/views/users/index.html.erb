<h1>Listing Users</h1>

<table>
  <tr>
    <th>Username</th>
    <th>Email</th>
    <th>Estate Agent</th>
  </tr>

<% @users.each do |user| %>
  <tr>
  	<td><%= user.username %></td>
  	<td><%= user.email %></td>
  	<td><%= user.estate_agent.name %></td>
  	<td><%= link_to 'Show', user %> | <%= link_to 'Edit', edit_user_path(user) %> | <%= link_to 'Destroy', user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New User', new_user_path %>
